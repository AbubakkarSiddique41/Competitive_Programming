#include <stdio.h>
int main(){
    int t; 
    scanf("%d",&t);
    for(int a0 = 0; a0 < t; a0++){
        long int n,sum=0,p; 
        scanf("%ld",&n);
        p = (n-1)/3;
        sum = ((3*p*(p+1))/2);

        p = (n-1)/5;
        sum = sum + ((5*p*(p+1))/2);

        p = (n-1)/15;
        sum = sum - ((15*p*(p+1))/2);
        printf("%ld\n",sum);
    }
    return 0;
}
